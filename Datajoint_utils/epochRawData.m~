function [timeAxis, data] = epochRawData(cell_id, epoch_numbers, channel)
if nargin < 3
    channel = 1;
end

Nepochs = length(epochNumbers);
data = cell(Nepochs,1);
T = cell(Nepochs,1);
L = zeros(Nepochs,1);
firstEpoch = sl.Epoch & sprintf('cell_id="%s"', cell_id) & sprintf('epoch_number=%d', epoch_numbers(1));
fname = [getenv('raw_data_folder'), firstEpoch.fetch1('raw_data_filename'), '.h5']

for i=1:Nepochs
    thisEpoch = sl.Epoch & sprintf('cell_id="%s"', cell_id) & sprintf('epoch_number=%d', epoch_numbers(i));
    ep_struct = thisEpoch.fetch('*');
    sample_rate = ep_struct.sample_rate;
    preTime = ep_struct.protocol_params.preTime;
    if channel==2
        data{i} = h5read(fname, ep_struct.data_link2);
    else
        data{i} = h5read(fname, ep_struct.data_link);
    end
    L = length(data{i})
    T{i} = 
end